<div id="data_display_panel" class="data_panel">
        <b>Data Collection : </b><span id='surburd-name'></span>
        <div id="panel-data">Hover over Melbourne area to get data</div>
</div>

<div id="feature_panel" class="data_panel">
      <b>Feature selection</b><br>
      <% @data_feature.each do |k,v|%>
        <input type="radio" name="feature" onclick='setFeatureData("<%= k %>")'> <%= v %><br>
      <% end %>
</div>

<div id="legend_panel" class="data_panel">
    <b>Feature legend</b>
    <div id="legend-data"></div>
</div>

<div id="map"></div>
<script 
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCkGOniAh0e3h5yJ1gcVP6ep1DqbIrUdzM&callback=initPolyMap">
</script>

<script src="http://google-maps-utility-library-v3.googlecode.com/svn/tags/markerwithlabel/1.1.5/src/markerwithlabel_packed.js"></script>